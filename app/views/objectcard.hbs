
<div class="container">

    <h2>Client params</h2>



    <form action="/objects/addObject" method="post">
        <div class="input-group">
            <span class="input-group-addon"><i class="glyphicon glyphicon-road"></i></span>
            <input id="id" type="text" class="form-control" name="id" readonly  placeholder="id" value="{{id}}">
        </div>

        <div class="input-group">
            <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
            <input id="name" type="text" class="form-control" name="name" placeholder="Name" value="{{name}}">
        </div>
        <div class="input-group">
            <span class="input-group-addon"><i class="glyphicon glyphicon-barcode"></i></span>
            <input id="code" type="number" min="0" class="form-control" name="code" placeholder="Code" value="{{code}}">
        </div>
